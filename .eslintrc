{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "eslint-config-prettier"],
  "rules": {
    "semi": [2, "never"],
    "no-console": 0
  }
}
